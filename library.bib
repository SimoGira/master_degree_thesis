keywords = {MR-compatible,MR-guided therapy,Remote-center-of-motion,Surgical robot},
number = {April},
pages = {1652--1656},
title = {{Needle guiding robot for MR-guided microwave thermotherapy of liver tumor using motorized remote-center-of-motion constraint}},
