keywords = {Actuators,Biomembranes,Electro Active Polymer,Electrodes,Force,Haptic display,Haptic interfaces,MR-compatible teleoperation,Medical robotics,Needle force display,Needles,Skin},
number = {8},
title = {{MR-Compatible Haptic Display of Membrane Puncture in Robot-Assisted Needle Procedures}},
volume = {14},
